#include <iostream>
#include <algorithm>
#include <math.h>
using namespace std;
int main()
{
	int a;
	cin >> a;
	if (a == 0) {
		cout << "zero";
	}
	else if (a > 0) {
		cout << "positive";
	}
	else {
		cout << "negative";
	}
	if (a != 0 and a % 2 == 0) {
		cout << " even number";
	}
	else if (a != 0 and a % 2 != 0) {
		cout << " odd number";
	}
}
