#include <iostream>
#include <algorithm>
#include <math.h>
using namespace std;
int main()
{
	int a;
	cin >> a;
	if (a == 0) {
		cout << "zero";
	}
	else if (a >= 1 and a <= 9) {
		cout << "single digit";
	}
	else if (a >= 10 and a <= 99){
		cout << "double digit";
	}
	else if (a >= 100 and a <= 999) {
		cout << "triple digit";
	}
	if (a != 0 and a % 2 == 0) {
		cout << " even number";
	}
	else if (a != 0 and a % 2 != 0) {
		cout << " odd number";
	}
}
