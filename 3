#include <iostream>
#include <algorithm>
#include <math.h>
using namespace std;
int main()
{
	int xa;
	int ya;
	int xb;
	int yb;
	int xc;
	int yc;
	cin >> xa >> ya >> xb >> yb >> xc >> yc;
	float ab = sqrt(pow(xb - xa, 2) + pow(yb - ya, 2));
	float ac = sqrt(pow(xc - xa, 2) + pow(yc - ya, 2));
	if (ab < ac) {
		cout << "b, ab = " << ab;
	}
	else {
		cout << "c, ac = " << ac;
	}
}
