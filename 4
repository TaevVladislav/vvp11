#include <iostream>
#include <algorithm>
#include <math.h>
using namespace std;
int main()
{
	int x;
	int y;
	cin >> x >> y;
	if (x > 0 and y > 0) {
		cout << "I";
	}
	else if (x < 0 and y > 0) {
		cout << "II";
	}
	else if (x < 0 and y < 0) {
		cout << "III";
	}
	else if (x > 0 and y < 0) {
		cout << "IV";
	}
}
